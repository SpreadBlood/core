<div class="container">
	<div class="col-sm-6 hidden-xs col-md-7 col-md-offset-2">
	<div id="charts"></div>
	</div>
	<div class="col-xs-12 col-sm-6 col-md-3">
		<div class="col-xs-12">
			<div class="row ">
				<%= image_tag "images/logo_normal.png", class: "col-xs-12" %>
				 <!-- Nav tabs -->
				  <ul class="nav nav-tabs" role="tablist">
				    <li role="presentation" class="active"><a href="#login" aria-controls="login" role="tab" data-toggle="tab">Login</a></li>
				    <li role="presentation"><a href="#register" aria-controls="register" role="tab" data-toggle="tab">Register</a></li>
				    <li role="presentation"><a href="#forgot" aria-controls="forgot" role="tab" data-toggle="tab">Forgot</a></li>
				  </ul>
								 <!-- Tab panes -->
				  <div class="tab-content">
				    <div role="tabpanel" class="tab-pane active" id="login">
				    	<%= render '/devise/sessions/new_form' %>
				    </div>
				    <div role="tabpanel" class="tab-pane" id="register">
						<%= render '/devise/registrations/new_form' %>
				    </div>
				    <div role="tabpanel" class="tab-pane" id="forgot">
				    	<%= render '/devise/passwords/new_form' %>
					</div>
			  </div>
			</div>
		</div>
	</div>
</div>

